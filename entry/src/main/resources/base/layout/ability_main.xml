<?xml version="1.0" encoding="utf-8"?>
<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:orientation="vertical"
    ohos:background_element="$color:background_color"
    >

    <ListContainer
        ohos:id="$+id:mListContainer"
        ohos:height="match_parent"
        ohos:width="match_parent"
        ohos:layout_alignment="horizontal_center"
        />
    <Button
        ohos:id="$+id:camera_btn"
        ohos:height="50vp"
        ohos:width="100vp"
        ohos:text="拍照"
        ohos:text_size="20vp"
        ohos:top_margin="20px"
        ohos:background_element="$graphic:button_bg_blue"
        />

</DependentLayout>