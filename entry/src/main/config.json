          {
            "actions": [
              "action.intent.INFO"
            ]
          }
        ]
      },
      {
        "orientation": "unspecified",
        "name": "com.yan.demo.CameraAbility",
        "icon": "$media:icon",
        "description": "$string:cameraability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "skills": [
          {
            "actions": [
              "action.intent.Camera"
            ]
          }
