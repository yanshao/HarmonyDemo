{
  "app": {
    "bundleName": "com.yan.demo",
    "vendor": "yan",
    "version": {
      "code": 1000000,
      "name": "1.0"
    },
    "apiVersion": {
      "compatible": 4,
      "target": 5,
      "releaseType": "Beta1"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.yan.demo",
    "name": ".MyApplication",
    "deviceType": [
      "phone"
